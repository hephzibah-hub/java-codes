
package jonasfolder;

import java.io.File;

public class JONASFOLDER {

   
    public static void main(String[] args) {
       
        String path = "c:\\JONASFOLDER";
        File file = new File (path);
        
        if(file.exists()){
            System.out.print("Folder is already created.") ; 

        }
    }
    
}
